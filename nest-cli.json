{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "assets": [{
        "include": "storage/upload-file/**/*",
        "outDir": "dist"
      },
      {
      "include": "mail/templates/**/*",
      "outDir": "dist"
    },
      {
        "include": "mail/assets/**/*",
        "outDir": "dist"
      }],
    "watchAssets": true
  }
}
